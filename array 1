public class array1 {
      public static void main(String[] args) {
            String[] buah = {"anggur","apel","mangga","pisang",};
            for( int i = 0; i < buah.length; i++ ){
            System.out.println("Buah" + buah[i] + ": ");
            //System.out.println("jumlah buah sebanyak:"+buah.length);
   }
            System.out.println("jumlah buah sebanyak:"+buah.length);
}
}

